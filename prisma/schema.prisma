generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id        String   @id @unique @default(uuid()) @db.Uuid
  title     String   @db.VarChar(255)
  synopsys  String   @db.Text
  year      Int      @db.Integer
  duration  Int      @db.Integer
}